{
  "name": "nuxt-boilerplate",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt cleanup && nuxt dev --host --clear --open",
    "generate": "nuxt generate",
    "preview": "nuxt preview",

    "lint:eslint": "eslint --fix",
    "lint:stylelint": "stylelint \"**/*.{css,vue}\" --fix",
    "lint-staged": "lint-staged",

    "test:run": "vitest run --passWithNoTests",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",

    "prepare": "husky || true",
    "postinstall": "nuxt prepare"
  },

  "dependencies": {
    "@nuxt/a11y": "^1.0.0-alpha.1",
    "@nuxt/content": "^3.11.2",
    "@nuxt/fonts": "^0.14.0",
    "@nuxt/image": "^2.0.0",
    "@nuxt/scripts": "^0.13.2",
    "@nuxtjs/i18n": "^10.2.3",
    "@nuxtjs/seo": "^3.4.0",
    "@pinia/nuxt": "^0.11.3",
    "@tailwindcss/vite": "^4.2.0",
    "@valibot/i18n": "^1.0.0",
    "@vee-validate/nuxt": "^4.15.1",
    "@vee-validate/valibot": "^4.15.1",
    "@vite-pwa/nuxt": "^1.1.1",
    "@vueuse/nuxt": "^14.2.1",
    "better-sqlite3": "^12.6.2",
    "maska": "^3.2.0",
    "motion-v": "^2.0.0-beta.4",
    "nuxt": "^4.3.1",
    "nuxt-swiper": "^2.0.1",
    "postcss-nested": "^7.0.2",
    "postcss-preset-env": "^11.1.3",
    "postcss-pxtorem": "^6.1.0",
    "reka-ui": "^2.8.0",
    "tailwindcss": "^4.2.0",
    "ufo": "^1.6.3",
    "valibot": "^1.2.0",
    "vite": "^7.3.1",
    "vue": "^3.5.28",
    "vue-router": "^4.6.4",
    "zod": "^4.3.6"
  },

  "devDependencies": {
    "@antfu/eslint-config": "^7.4.3",
    "@dreamsicle.io/stylelint-config-tailwindcss": "^1.2.2",
    "@nuxt/eslint": "^1.15.1",
    "@nuxt/hints": "^1.0.0-alpha.7",
    "@nuxt/test-utils": "^4.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^10.0.0",
    "eslint-plugin-vuejs-accessibility": "^2.5.0",
    "happy-dom": "^20.6.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "stylelint": "^17.3.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^40.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },

  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "sqlite3"
    ],

    "overrides": {
      "@eslint-community/eslint-plugin-eslint-comments": "^4.6.0",
      "@nuxt/cli": {
        "@bomb.sh/tab": {
          "citty": "^0.2.1"
        }
      },
      "eslint": "$eslint",
      "eslint-plugin-jsdoc": "^50.6.1",
      "eslint-plugin-pnpm": "^1.5.0",
      "glob": "^13.0.6",
      "source-map": "^0.7.6",
      "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@1.5.5",
      "swiper": "^12.1.2",
      "vite": "^7.3.1",
      "vue-i18n": "^11.1.11"
    }
  },

  "overrides": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.6.0",
    "@nuxt/cli": {
      "@bomb.sh/tab": {
        "citty": "^0.2.1"
      }
    },
    "eslint": "$eslint",
    "eslint-plugin-jsdoc": "^50.6.1",
    "eslint-plugin-pnpm": "^1.5.0",
    "glob": "^13.0.6",
    "source-map": "^0.7.6",
    "sourcemap-codec": "npm:@jridgewell/sourcemap-codec@1.5.5",
    "swiper": "^12.1.2",
    "vite": "^7.3.1",
    "vue-i18n": "^11.1.11"
  },

  "lint-staged": {
    "*": "eslint --fix",
    "*.{css,vue}": "stylelint --fix"
  }
}
